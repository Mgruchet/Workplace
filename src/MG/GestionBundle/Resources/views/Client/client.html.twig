{% extends '::base.html.twig' %}
{% block body %}
{% trans_default_domain 'FOSUserBundle' %}

{% include 'header.html.twig' %}
  <section>
    {% include 'aside.html.twig' %}
    <div class="container">
      <section class="contenue--client">
        <article class="show--client">
          <h3 class="title--client">Mes Clients</h3>
          {% for client in clients %}
            <p class="text--client">{{client.nom}} {{client.prenom}}</p>
          {% endfor %}
        </article>
      </section>

    </div>
  </section>

{% endblock %}
